<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>false</PackageRequireLicenseAcceptance>
    <Authors>Trevor Tubbs</Authors>
    <Description>A simple factory library for instantiating objects.</Description>
    <RepositoryUrl>https://github.com/TrevorTubbs/factory</RepositoryUrl>
    <Product>Simple Factory</Product>
    <Title>tt.factory</Title>
    <Version>1.3.1</Version>
    <AssemblyVersion>1.3.1.0</AssemblyVersion>
    <FileVersion>1.3.1.0</FileVersion>
    <PackageReleaseNotes>Added support for .NET Core.</PackageReleaseNotes>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Debug|AnyCPU'">
    <DocumentationFile>bin\Debug\netstandard2.0\task.factory.xml</DocumentationFile>
  </PropertyGroup>

</Project>
